{
  "name": "labelify-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Labelify Frontend",
  "main": "index.js",
  "scripts": {
    "start": "npx parcel public/index.html",
    "build": "npx parcel build public/index.html",
    "fix": "npm run tslint --fix && prettier --write ./src/**/*.ts",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint": "npm run tslint && prettier -l ./src/**/*.ts",
    "test": "jest",
    "test:ci": "jest -c ./jest.config.ci.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@material-ui/core": "^3.6.2",
    "@storybook/addon-actions": "^4.0.12",
    "@storybook/addon-links": "^4.0.12",
    "@storybook/addons": "^4.0.12",
    "@storybook/react": "^4.0.12",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.13",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__react": "^4.0.0",
    "babel-loader": "^8.0.4",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.15.3",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@material-ui/icons": "^3.0.1",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run fix",
      "npm run lint",
      "git add"
    ]
  }
}
